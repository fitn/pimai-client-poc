{
  "name": "akeneo/pim-ai-api-php-client",
  "description": "Pim.ai PHP client for the API",
  "license": "OSL-3.0",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Akeneo",
      "homepage": "http://www.akeneo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Akeneo\\PimAi\\ApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Akeneo\\PimAi\\ApiClient\\tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6.3",
    "symfony/http-foundation": "@stable"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v2.3",
    "phpunit/phpunit": "5.7.*",
    "phpspec/phpspec": "3.2.*",
    "symfony/yaml": "^3.3"
  }
}
